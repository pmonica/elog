<div class="row">
  <div class="col-sm-12">
      <%= link_to 'New Tag', tags_new_path, :type => 'button', :class => 'btn btn-primary'%>
      <hr>
    <table class="table table-striped">
      <thead>
        <tr><th></th><th>Action</th><th>Action</th></tr>
      </thead>
      <tbody>
        <% @tags.each do |t| %>
          <tr>
            <td> <%=t.body %></td>
            <td><%= link_to 'Delete', tags_destroy_path(t) %></td>
            <td><%= link_to 'Edit', tags_edit_path(t) %></td>
          </tr>
        <% end %> <!-- end do --> 
      </tbody>
    </table>
  </div> <!-- end da sm-12  -->
</div>