<div class="container">
  <div class="row">
    <h3>Users</h3>
    <!-- <div class="column"> -->
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <% if !((user.email.include? "user_") && (user.email.include? "_deactivated")) %>
                 <%= render user %>
              <% end %>
            </tr>
          <% end %>
        </tbody>
      </table>
    <!-- </div> -->
  </div>
</div>
